{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "proposedChargeId": {
      "type": "string"
    },
    "version": {
      "type": "integer"
    },
    "rejectionReason": {
      "type": "string",
      "enum": [
        "Evidential",
        "Public Interest",
        "Covered by Other Charges"
      ]
    }
  },
  "required": [
    "proposedChargeId",
    "version",
    "rejectionReason"
  ]
}